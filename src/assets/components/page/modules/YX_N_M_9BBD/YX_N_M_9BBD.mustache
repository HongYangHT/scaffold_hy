<div class="YX-N-M-9BBD m-section-module" style="background-image:url({{contentInfo.backgroundImage}})" data-h55970f92="backgroundImage">
	<div class="J_module_info f-hide">
		<div class="goodsId" data-goodsId="{{contentInfo.goodsId}}"></div>
	</div>
    <div class="m-9BBD-goods">
		<div class="m-slider swiper-container" id="goodsId{{contentInfo.goodsId}}">
			<ul class="m-goods-list f-cl-b swiper-wrapper">
                {{#temp}}
                <li class="swiper-slide f-inline">
                    <div class="u-item">
                        <div class="u-item-img">
                            <div class="u-icon">
                                <i class="u-tip-icon">{{extend.tip_key}}</i>
                            </div>
                            <a href="http://you.163.com/item/detail?id={{id}}&_stat_subject=" class="PSC_J_normal_statistics_Goods" target="_blank">
                                <img class="lazy" src="{{primaryPicUrl}}" style="display: block;">
                            </a>
                        </div>
                        <div class="u-item-name">
                            <span class="f-fontSize-16">{{title}}</span>
                            <span class="f-fontSize-14">{{simpleDesc}}</span>
                        </div>
                        <div class="u-item-prize">
                            <span>
                                <i class="u-yen">{{retailPrice}}</i>
                                <i class="u-del">{{unitPrice}}</i>
                            </span>
                            <button type="button" data-skuid="{{id}}" data-img="{{primaryPicUrl}}" class="u-btn J-addCart PSC_J_normal_statistics_Goods">加入购物车</button>
                        </div>
                    </div>
                </li>
                {{/temp}}        
            </ul>
			<a href="javascript:;" target="_self" class="swiper-button-prev slider-arrow u-prev swiper-button-disabled J_prev{{contentInfo.goodsId}}"></a>
            <a href="javascript:;" target="_self" class="swiper-button-next slider-arrow u-next J_next{{contentInfo.goodsId}}"></a>
        </div>
    </div>
</div>