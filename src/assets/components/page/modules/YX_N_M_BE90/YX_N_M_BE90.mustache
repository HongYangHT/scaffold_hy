<div class="YX-N-M-BE90 m-section-module" style="background-color: {{contentInfo.bgColor}};" data-h55970f92="bgColor">
	<div class="J_module_info f-hide" style="display: none;">
		<div class="goodsId" data-goodsId="{{contentInfo.goodsId}}"></div>
		<div class="needScenePicUrl" data-needScenePicUrl="{{contentInfo.needScenePicUrl}}"></div>
		<div class="needGoodsNum" data-needGoodsNum="{{contentInfo.needGoodsNum}}"></div>
	</div>
	<div class="m-BE90-inner">
		<div class="u-BE90-img" data-h55970f92="leftImg">
			<img src="{{contentInfo.leftImg}}" data-original="{{contentInfo.leftImg}}" class="J_lazyload">
		</div>
		<div class="m-BE90-goods" v-if="contentInfo.needScenePicUrl.value" style="background-color: {{contentInfo.bgColor}};">
			{{#temp}}
			<div class="u-BE90-goods">
				<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-img-link">
					<img src="{{scenePicUrl}}" data-original="{{scenePicUrl}}" class="J_lazyload">
				</a>
				<p class="u-BE90-title"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-title-link">{{title}}</a></p>
				{{#maker}}<p class="u-BE90-desc">{{maker}}</p>{{/maker}}
                    {{^maker}}<p class="u-BE90-desc">{{simpleDesc}}</p>{{/maker}}
				<p class="u-BE90-price">
					<span class="u-BE90-offPrice">{{offPrice}}</span>
					{{#unitPrice}}<span class="u-BE90-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
				</p>
				<div class="m-BE90-btn">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-buy">立即购买</a>
					<a href="javascript:;" target="_self" class="u-BE90-cart J_BE90_cart"></a>
				</div>
			</div>
			{{/temp}}
		</div>
		<div class="m-BE90-goods" v-if="!contentInfo.needScenePicUrl.value" style="background-color: {{contentInfo.bgColor}};">
			{{#temp}}
			<div class="u-BE90-goods">
				<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-img-link">
					<img src="{{primaryPicUrl}}" data-original="{{primaryPicUrl}}" class="J_lazyload">
				</a>
				<p class="u-BE90-title"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-title-link">{{title}}</a></p>
				{{#maker}}<p class="u-BE90-desc">{{maker}}</p>{{/maker}}
                    {{^maker}}<p class="u-BE90-desc">{{simpleDesc}}</p>{{/maker}}
				<p class="u-BE90-price">
					<span class="u-BE90-offPrice">{{offPrice}}</span>
					{{#unitPrice}}<span class="u-BE90-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
				</p>
				<div class="m-BE90-btn">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-BE90-buy">立即购买</a>
					<a href="javascript:;" target="_self" class="u-BE90-cart J_BE90_cart"></a>
				</div>
			</div>
			{{/temp}}
		</div>
	</div>
</div>