<div class="YX-N-M-CB4D m-section-module" style="background-color:{{contentInfo.bgColor}};" data-h55970f92="bgColor">
	<div class="J_module_info f-hide" style="display: none;">
		<div class="goodsId" data-goodsId="{{contentInfo.goodsId}}"></div>
		<div class="needScenePicUrl" data-needScenePicUrl="{{contentInfo.needScenePicUrl}}"></div>
		<div class="needGoodsNum" data-needGoodsNum="{{contentInfo.needGoodsNum}}"></div>
	</div>
	<div class="m-CB4D-inner">
		<div class="m-CB4D-left">
			<div class="m-CB4D-goods" v-if="contentInfo.needScenePicUrl.value">
				{{#temp}}
				<div class="u-CB4D-goods">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-img-link">
						<img src="{{scenePicUrl}}" data-original="{{scenePicUrl}}" class="J_lazyload">
					</a>
					<p class="u-CB4D-title"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-title-link">{{title}}</a></p>
					{{#maker}}<p class="u-CB4D-desc">{{maker}}</p>{{/maker}}
                    {{^maker}}<p class="u-CB4D-desc">{{simpleDesc}}</p>{{/maker}}
					<div class="u-CB4D-border"></div>
					<p class="u-CB4D-price">
						<span class="u-CB4D-offPrice">{{offPrice}}</span>
						{{#unitPrice}}<span class="u-CB4D-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
					</p>
					<div class="m-CB4D-btn">
						<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-buy">立即购买</a>
						<a href="javascript:;" target="_self" class="u-CB4D-cart J_CB4D_cart"></a>
					</div>
				</div>
				{{/temp}}
			</div>
			<div class="m-CB4D-goods" v-if="!contentInfo.needScenePicUrl.value">
				{{#temp}}
				<div class="u-CB4D-goods">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-img-link">
						<img src="{{primaryPicUrl}}" data-original="{{primaryPicUrl}}" class="J_lazyload">
					</a>
					<p class="u-CB4D-title"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-title-link">{{title}}</a></p>
					{{#maker}}<p class="u-CB4D-desc">{{maker}}</p>{{/maker}}
                    {{^maker}}<p class="u-CB4D-desc">{{simpleDesc}}</p>{{/maker}}
					<div class="u-CB4D-border"></div>
					<p class="u-CB4D-price">
						<span class="u-CB4D-offPrice">{{offPrice}}</span>
						{{#unitPrice}}<span class="u-CB4D-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
					</p>
					<div class="m-CB4D-btn">
						<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-CB4D-buy">立即购买</a>
						<a href="javascript:;" target="_self" class="u-CB4D-cart J_CB4D_cart"></a>
					</div>
				</div>
				{{/temp}}
			</div>
		</div>
		<div class="m-CB4D-right" data-h55970f92="sideImg">
			<img src="{{contentInfo.sideImg}}" data-original="{{contentInfo.sideImg}}" class="J_lazyload">
		</div>
	</div>
</div>					