<div class="YX-N-M-417A m-section-module">
	<div class="J_module_info f-hide" style="display: none;">
		<div class="goodsId" data-goodsId="{{contentInfo.goodsId}}"></div>
		<div class="needScenePicUrl" data-needScenePicUrl="{{contentInfo.needScenePicUrl}}"></div>
	</div>
	<div class="m-417A-inner">
		<div class="u-417A-goods-tit" data-h55970f92="titurl">
            <a href="{{contentInfo.titurl}}" target="_blank" class="u-417A-link" data-h55970f92="titimg"><img data-original="{{contentInfo.titimg}}" class="lazy" src="{{contentInfo.titimg}}"></a>
        </div>
		<div class="m-417A-goods" v-if="contentInfo.needScenePicUrl.value">
			{{#temp}}
				<div class="u-417A-goods">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-417A-link">
						<img src="{{scenePicUrl}}" data-original="{{scenePicUrl}}" class="J_lazyload">
					</a>
					<div class="m-417A-goods-info">
						<p class="u-417A-goods-t"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank">{{title}}</a></p>
						{{#maker}}<p class="u-417A-goods-maker">{{maker}}</p>{{/maker}}
						{{^maker}}<p class="u-417A-goods-maker">{{simpleDesc}}</p>{{/maker}}
						<p class="u-417A-goods-price">
							<span class="u-417A-offPrice">{{offPrice}}</span>
							{{#unitPrice}}<span class="u-417A-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
						</p>
						<div class="m-417A-btn">
							<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-417A-btn">立即购买</a>
							<a href="javascript:;" target="_self" class="u-417A-cart J_417A_cart"></a>
						</div>
					</div>
				</div>
			{{/temp}}
		</div>
		<div class="m-417A-goods" v-if="!contentInfo.needScenePicUrl.value">
			{{#temp}}
				<div class="u-417A-goods">
					<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-417A-link">
						<img src="{{primaryPicUrl}}" data-original="{{primaryPicUrl}}" class="J_lazyload">
					</a>
					<div class="m-417A-goods-info">
						<p class="u-417A-goods-t"><a href="http://you.163.com/item/detail?id={{id}}" target="_blank">{{title}}</a></p>
						{{#maker}}<p class="u-417A-goods-maker">{{maker}}</p>{{/maker}}
						{{^maker}}<p class="u-417A-goods-maker">{{simpleDesc}}</p>{{/maker}}
						<p class="u-417A-goods-price">
							<span class="u-417A-offPrice">{{offPrice}}</span>
							{{#unitPrice}}<span class="u-417A-unitPrice">{{unitPrice}}</span>{{/unitPrice}}
						</p>
						<div class="m-417A-btn">
							<a href="http://you.163.com/item/detail?id={{id}}" target="_blank" class="u-417A-btn">立即购买</a>
							<a href="javascript:;" target="_self" class="u-417A-cart J_417A_cart"></a>
						</div>
					</div>
				</div>
			{{/temp}}
		</div>
	</div>
</div>